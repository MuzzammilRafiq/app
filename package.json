{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && NODE_ENV=development electron .",
    "build:react": "tsc -b && vite build",
    "build:electron": "npm run transpile:electron",
    "build": "npm run transpile:electron && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "npm run format && npm run lint:fix",
    "preview": "vite preview",
    "transpile:electron": "tsc -p src/electron/tsconfig.json && tsc -p src/electron/tsconfig.preload.json && mv dist-electron/ipc/preload.js dist-electron/preload.cjs",
    "dist:mac": "npm run build:electron && npm run build:react && electron-builder --mac --arm64",
    "start": "npm run dev:electron",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "rebuild": "electron-rebuild",
    "lancedb:example": "bun run lancedb-example.ts"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.5",
    "@electron/rebuild": "^4.0.1",
    "@google/genai": "^1.6.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-table": "^8.21.3",
    "@types/bun": "^1.2.20",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "googleapis": "^150.0.1",
    "groq-sdk": "^0.27.0",
    "heic2any": "^0.0.4",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
