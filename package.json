{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && NODE_ENV=development electron .",
    "build:react": "tsgo -b && vite build",
    "build:electron": "npm run transpile:electron",
    "build": "npm run transpile:electron && tsgo -b && vite build",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format:fix": "npm run format:write && npm run lint:fix",
    "preview": "vite preview",
    "transpile:electron": "tsgo -p src/electron/tsconfig.json && tsgo -p src/electron/tsconfig.preload.json && mv dist-electron/ipc/preload.js dist-electron/preload.cjs",
    "dist:mac": "npm run build:electron && npm run build:react && electron-builder --mac --arm64",
    "start": "npm run dev:electron",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "rebuild": "electron-rebuild",
    "lancedb:example": "bun run lancedb-example.ts"
  },
  "dependencies": {
    "@electron/rebuild": "^4.0.1",
    "@google/genai": "^1.6.0",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@openrouter/sdk": "^0.3.10",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "@types/bun": "^1.2.20",
    "baseline-browser-mapping": "^2.9.11",
    "caniuse-lite": "^1.0.30001761",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "googleapis": "^150.0.1",
    "groq-sdk": "^0.27.0",
    "heic2any": "^0.0.4",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript/native-preview": "^7.0.0-dev.20251225.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "globals": "^16.0.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.35.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.9.2",
    "vite": "^6.3.5"
  }
}
